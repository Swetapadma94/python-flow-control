###prime number     
     for n in range(2, 10):
    for x in range(2,n):
        if n % x == 0:
            print(n, 'equals', x, '*', n//x)
            break
    else:
        print(n, 'is a prime number')

# continue statement
for i in range(1,10):
    if i % 2 ==0:
        print("Even Number",i)
        continue
    print("odd number",i) 
    
    # # user defined function
# to print fibonacci series
def fib1(n):
    result=[]
    a,b=0,1
    while(a<n):
        result.append(a)
        a,b=b,a+b
    return result

fib1(10)

# unpacking argument list
args=[3,6]
print(list(range(*args)))
