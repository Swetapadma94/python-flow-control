# flow control
#while
a,b= 0,1
while(a<5):
    print(a)
    a,b=b,a+b
    
    # if-Else Statement
x= int(input("enter an integer:"))
if x<0:
    print("Negative")
elif x == 0:
    print("Zero")
else:
    print("positive")
    
    # for Loop
animal=["cat","cow","dog","tiger","calf"]
for w in animal:
    print(w,len(w))
    
    # range functin(iterate over a sequence of numbers)

for i in range(5):
    print(i)
    
    for i in range(0,23,2):
    print(i)
    
